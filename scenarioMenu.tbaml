<?xml version="1.0" encoding="utf-8"?>
<TangibleBoxApp xmlns="http://www.example.com"
    xmlns:tba="http://www.w3.org/2001/XMLSchema-instance"
    tba:schemaLocation="./tba.xsd">
	<Panel
	    tba:layout_width="800"
	    tba:layout_height="">
		
		<Pions>
	    	<Pion 
	    		tba:id="pionFrise"
	    		tba:color="red"
	    		tba:form="rond"/>
	    		
	    		<Pion 
	    		tba:id="pionMenu"
	    		tba:color="yellow"
	    		tba:form="rond"/>
	    	</Pions>
	    	
	    <ZoneGroup 
		    tba:id=""/>
		
		<Zones>
		
		<!-- -------------Zones Frise---------------------- -->
			<Zone  
		    	tba:id="zonePleiade"
		    	tba:idGroup="null"
				tba:form="rect" 
		    	tba:x="110"
		    	tba:y="100"
		    	tba:width="30"
		    	tba:height="20"
		   	 	tba:texte="Pléïade"/>
		   	 	
		   	 	<Zone   
		    	tba:id="zoneBaroque"
		    	tba:idGroup="null"
				tba:form="rect" 
		    	tba:x="140"
		    	tba:y="100"
		    	tba:width="30"
		    	tba:height="20"
		   	 	tba:texte="Baroque"/>
		   	 	
		   	 	<Zone    
		    	tba:id="zoneLumieres"
		    	tba:idGroup="null"
				tba:form="rect" 
		    	tba:x="170"
		    	tba:y="100"
		    	tba:width="30"
		    	tba:height="20"
		   	 	tba:texte="Lumières"/>
		   	 	
		   	 	<Zone    
		    	tba:id="zoneRomantisme"
		    	tba:idGroup="null"
				tba:form="rect" 
		    	tba:x="200"
		    	tba:y="100"
		    	tba:width="30"
		    	tba:height="20"
		   	 	tba:texte="Romantisme"/>
		   	 	
		   	 	<Zone      
		    	tba:id="zoneSurrealisme"
		    	tba:idGroup="null"
				tba:form="rect" 
		    	tba:x="230"
		    	tba:y="100"
		    	tba:width="30"
		    	tba:height="20"
		   	 	tba:texte="Surrealisme"/>
		   	 	
		   	 	<!-- -------------Zones Menu---------------------- -->
		   	 	
		   	 	<Zone      
		    	tba:id="zoneMenuAuteur"
		    	tba:idGroup="null"
				tba:form="rect" 
		    	tba:x="140"
		    	tba:y="130"
		    	tba:width="30"
		    	tba:height="20"
		   	 	tba:texte="auteur"/>
		   	 	
		   	 	<Zone      
		    	tba:id="zoneMenuPeintre"
		    	tba:idGroup="null"
				tba:form="rect" 
		    	tba:x="110"
		    	tba:y="130"
		    	tba:width="30"
		    	tba:height="20"
		   	 	tba:texte="peintre"/>
		   	 	
		   	 	
		   	 	
		   	</Zones>
		   	
		   	<Texts>
		   		<Text
		   		tba:id="textPeintreBaroque"
		   		tba:texte="Rembrandt"/>
		   		
		   		<Text
		   		tba:id="textAuteurBaroque"
		   		tba:texte="Christofle de Beaujeu"/>
		   		
		   		<Text
		   		tba:id="textPeintrePleiade"
		   		tba:texte="Leonard De Vinci"/>
		   		
		   		<Text
		   		tba:id="textAuteurPleiade"
		   		tba:texte="Rabelais"/>
		   		
		   		<Text
		   		tba:id="textPeintreRomantisme"
		   		tba:texte="Théodore Géricault"/>
		   		
		   		<Text
		   		tba:id="textAuteurRomantisme"
		   		tba:texte="Victor Hugo"/>
		   		
		   		<Text
		   		tba:id="textPeintreLumiere"
		   		tba:texte="Jean-Antoine Watteau"/>
		   		
		   		<Text
		   		tba:id="textAuteurLumiere"
		   		tba:texte="Voltaire"/>
		   		
		   		<Text
		   		tba:id="textPeintreSurrealisme"
		   		tba:texte="Salvador Dali"/>
		   		
		   		<Text
		   		tba:id="textAuteurSurrealisme"
		   		tba:texte="André Breton"/>
		   	</Texts>		   	
		   	
		   	
	</Panel>
	
	<Behavior>
	
		<Conds>
			<VarCond
		   	tba:id="conditionAuteurPleiade"
		   	tba:variable="vaMenu"
		   	tba:valCible="auteur"/>
		   		
		   	<VarCond
		   	tba:id="conditionPeintrePleiade"
		   	tba:variable="valMenu"
		   	tba:valCible="peintre"/>
		   		
		   	<VarCond
		   	tba:id="conditionAuteurBaroque"
		   	tba:variable="valMenu"
		   	tba:valCible="auteur"/>
		   	
		   	<VarCond
		   	tba:id="conditionPeintreBaroque"
		   	tba:variable="valMenu"
		   	tba:valCible="peintre"/>
		   	
		   	<VarCond
		   	tba:id="conditionAuteurSurrealisme"
		   tba:variable="valMenu"
		   	tba:valCible="auteur"/>
		   	
		   	<VarCond
		   	tba:id="conditionPeintreSurrealisme"
		   	tba:variable="valMenu"
		   	tba:valCible="peintre"/>
		   	
		   	<VarCond
		   	tba:id="conditionAuteurLumieres"
		   	tba:variable="valMenu"
		   	tba:valCible="auteur"/>
		   	
		   	<VarCond
		   	tba:id="conditionPeintreLumieres"
		   	tba:variable="valMenu"
		   	tba:valCible="peintre"/>
		   	
		   	<VarCond
		   	tba:id=" conditionAuteurRomantisme"
		   	tba:variable="valMenu"
		   	tba:valCible="auteur"/>
		   		
		   	<VarCond
		   	tba:id=" conditionPeintreRomantisme"
		   	tba:variable="valMenu"
		   	tba:valCible="peintre"/>	
		   	
		</Conds>
		
		<Vars>
			<Var 
				tba:id="valMenu"/>
		</Vars>
	</Behavior>
	<Output>
	
		<Tts
		tba:id="maTTS"/>
	</Output>
	
	<Links>
		<!-- -------------Liens PIONS A ZONES---------------------- -->
		<Link
		tba:srcId="pionFrise"
		tba:destId="zoneSurrealisme"/>
			
		<Link
		tba:srcId="pionFrise"
		tba:destId="zonePleiade"/>
			
		<Link
		tba:srcId="pionFrise"
		tba:destId="zoneBaroque"/>
			
		<Link
		tba:srcId="pionFrise"
		tba:destId="zoneLumieres"/>
		
		<Link
		tba:srcId="pionFrise"
		tba:destId="zoneRomantisme"/>
		
		<Link
		tba:srcId="pionMenu"
		tba:destId="zoneMenuAuteur"/>	
		
		<Link
		tba:srcId="pionMenu"
		tba:destId="zoneMenuPeintre"/>				
			
			
			
			
		<!-- --------------Liens ZONES A CONDITIONS------------------- -->
			<Link
			tba:srcId="zonePleiade"
			tba:destId="conditionAuteurPleiade"/>
			
			<Link
			tba:srcId="zonePleiade"
			tba:destId="conditionPeintrePleiade"/>
			
			<Link
			tba:srcId="zoneBaroque"
			tba:destId="conditionAuteurBaroque"/>
			
			<Link
			tba:srcId="zoneBaroque"
			tba:destId="conditionPeintreBaroque"/>
			
			<Link
			tba:srcId="zoneRomantisme"
			tba:destId="conditionAuteurRomantisme"/>
			
			<Link
			tba:srcId="zoneRomantisme"
			tba:destId="conditionPeintreRomantisme"/>
			
			<Link
			tba:srcId="zoneSurrealisme"
			tba:destId="conditionAuteurSurrealisme"/>
			
			<Link
			tba:srcId="zoneSurrealisme"
			tba:destId="conditionPeintreSurrealisme"/>
			
			<Link
			tba:srcId="zoneLumieres"
			tba:destId="conditionAuteurLumieres"/>
			
			<Link
			tba:srcId="zoneLumieres"
			tba:destId="conditionPeintreLumieres"/>

		<!-- --------Liens CONDITIONS A TEXTE-------------------- -->
		
			<Link
			tba:srcId="conditionAuteurBaroque"
			tba:destId="textAuteurBaroque"/>
			
			<Link
			tba:srcId="conditionPeintreBaroque"
			tba:destId="textPeintreBaroque"/>
			
			<Link
			tba:srcId="conditionAuteurPleiade"
			tba:destId="textAuteurPleiade"/>
			
			<Link
			tba:srcId="conditionPeintrePleiade"
			tba:destId="textPeintrePleiade"/>
			
			<Link
			tba:srcId="conditionAuteurLumieres"
			tba:destId="textAuteurLumieres"/>
			
			<Link
			tba:srcId="conditionPeintreLumieres"
			tba:destId="textPeintreLumieres"/>
			
			<Link
			tba:srcId="conditionAuteurRomantisme"
			tba:destId="textAuteurRomantisme"/>
			
			<Link
			tba:srcId="conditionPeintreRomantisme"
			tba:destId="textPeintreRomantisme"/>
			
			<Link
			tba:srcId="conditionAuteurSurrealisme"
			tba:destId="textAuteurSurrealisme"/>
			
			<Link
			tba:srcId="conditionPeintreSurrealisme"
			tba:destId="textPeintreSurrealisme"/>
			
			<Link
			tba:srcId="zoneMenuAuteur"
			tba:destId="valMenu"/>
			
			<Link
			tba:srcId="zoneMenuAuteur"
			tba:destId="valMenu"/>
			
		<!-- --------Liens TEXTE A TTS-------------------- -->

			<Link
			tba:srcId="textPeintreBaroque"
			tba:destId="maTTS"/>
			
			<Link
			tba:srcId="textPeintrePleiade"
			tba:destId="maTTS"/>
			
			<Link
			tba:srcId="textAuteurBaroque"
			tba:destId="maTTS"/>
			
			<Link
			tba:srcId="textAuteurPleiade"
			tba:destId="maTTS"/>
			
			<Link
			tba:srcId="textAuteurLumiere"
			tba:destId="maTTS"/>
			
			<Link
			tba:srcId="textAuteurRomantisme"
			tba:destId="maTTS"/>
			
			<Link
			tba:srcId="textAuteurSurrealisme"
			tba:destId="maTTS"/>
			
			<Link
			tba:srcId="textPeintreLumiere"
			tba:destId="maTTS"/>
			
			<Link
			tba:srcId="textPeintreRomantisme"
			tba:destId="maTTS"/>
			
			<Link
			tba:srcId="textPeintreSurrealisme"
			tba:destId="maTTS"/>		
			
			

			
	</Links>
	
</TangibleBoxApp>